// Problem code INTEST



#include <iostream>
using namespace std;

int main(){
    int input; //7
    int divisor; //3
    int a; //number of input value 
    int count;
    count=0;
    
    cin>>input; //7
    cin>>divisor; //3
    
    for(int i =0; i<input; i++){
        
        cin>>a; 
        if(a%divisor == 0){
           count++; 
        }
        
        
    }
    cout<<count;
}
